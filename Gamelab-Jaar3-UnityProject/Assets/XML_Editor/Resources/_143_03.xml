  <List>
    <Element icn="Step5_EnterTR">
      <lines>
        <Line>
          <actors>
            <actor>Ana</actor>
          </actors>
          <text>Welcome to the teaching room, patient 929.</text>
          <expression>
            <portraitExpression>None</portraitExpression>
            <voiceActing>false</voiceActing>
          </expression>
          <cameraPosition>
            <move>Middle</move>
          </cameraPosition>
          <choices />
