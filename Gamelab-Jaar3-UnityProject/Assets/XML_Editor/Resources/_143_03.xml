    <Element icn="FreeTimeBlaze_01">
      <lines>
        <Line>
          <actors>
            <actor>Blaze</actor>
          </actors>
          <text>Yo.</text>
          <expression>
            <portraitExpression>Neutral</portraitExpression>
            <voiceActing>true</voiceActing>
