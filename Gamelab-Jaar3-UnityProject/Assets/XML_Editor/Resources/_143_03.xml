            <Effect>
              <effect>PlaySFX</effect>
              <Parameters>
                <string>VOICE_WelcomeToTheTeachingRoom</string>
              </Parameters>
            </Effect>
          </effect>
        </Line>
        <Line>
          <actors>
            <actor>Ana</actor>
          </actors>
          <text>Please find a seat and fill out this survey, afterwards, you are free to roam the facility.</text>
          <expression>
            <portraitExpression>None</portraitExpression>
            <voiceActing>false</voiceActing>
          </expression>
          <cameraPosition>
            <move>Middle</move>
          </cameraPosition>
          <choices />
          <effect />
        </Line>
