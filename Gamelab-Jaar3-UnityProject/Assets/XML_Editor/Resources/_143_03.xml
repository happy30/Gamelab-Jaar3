    <Element icn="Step5_Survey">
      <lines>
        <Line>
          <actors>
            <actor>Mind</actor>
          </actors>
          <text>Question 1: Do you remember anything prior to waking up here?</text>
          <expression>
            <portraitExpression>None</portraitExpression>
            <voiceActing>false</voiceActing>
          </expression>
          <cameraPosition>
            <move>Middle</move>
          </cameraPosition>
          <choices>
            <Choices>
